{
		"opts": {
				"destination": "docs",
				"encoding": "utf8",
				"package": "package.json",
				"recurse": true,
				"template": "node_modules/tui-jsdoc-template"
		},
		"plugins": [ "plugins/markdown" ],
		"templates": {
			"default": {
				"staticFiles": {
						"include": ["config/jsdoc/styles"]
					}
			},
			"css": [
					"custom.css"
			],
			"name": "YUKA"
		}
}
